    "autoprefixer": "10.4.15",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "daisyui": "^4.5.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.18",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "file-type": "^18.5.0",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.11",
    "formidable": "^3.5.1",
    "formidable-serverless": "^1.1.1",
    "lottie-react": "^2.4.1",
    "mime": "^3.0.0",
    "nanoid": "^5.0.3",
    "next": "13.4.18",
    "next-auth": "^4.23.1",
    "nextjs-cors": "^2.2.0",
    "node-mocks-http": "^1.13.0",
    "paseto": "^3.1.4",
    "pg": "^8.11.3",
    "postcss": "8.4.28",
    "prisma": "^6.14.0",
    "react": "18.2.0",
    "react-beforeunload": "^2.6.0",
    "react-daisyui": "^5.0.0",
    "react-dom": "18.2.0",
    "react-filepond": "^7.1.2",
    "react-hook-form": "^7.49.3",
    "read-chunk": "^4.0.3",
